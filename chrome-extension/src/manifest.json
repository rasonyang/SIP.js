{
  "manifest_version": 3,
  "name": "SIP.js Headless Extension",
  "version": "1.0.0",
  "description": "Headless SIP.js Chrome Extension with BroadSoft support - Reference Implementation",
  "permissions": [
    "storage",
    "alarms",
    "offscreen"
  ],
  "host_permissions": [
    "wss://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "icons": {
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  }
}
